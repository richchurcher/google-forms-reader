{
  "name": "google-forms-reader",
  "version": "0.1.0",
  "description": "Grab and organise Google Forms data from the associated Google Sheet",
  "main": "lib/index.js",
  "scripts": {
    "transpile": "babel src --out-dir lib",
    "prepublish": "npm run transpile",
    "test": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richchurcher/google-forms-reader.git"
  },
  "keywords": [
    "google",
    "forms",
    "read",
    "sheets"
  ],
  "author": "Rich Churcher",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/richchurcher/google-forms-reader/issues"
  },
  "homepage": "https://github.com/richchurcher/google-forms-reader#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1"
  },
  "dependencies": {
    "dotenv": "^2.0.0",
    "google-auth-library": "^0.9.10",
    "googleapis": "^14.2.0",
    "ramda": "^0.22.1"
  },
  "files": [
    "lib"
  ],
  "ava": {
    "tap": true,
    "require": [
      "babel-register",
      "dotenv/config"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 3.0.0"
  },
  "lambci": {
    "cmd": "nave use 7 bash -c 'npm install && npm test'"
  }
}
